
<div class="container" ng-app="mainApp" ng-controller="LoginController">
        <h1>SignUp</h1>
        <p>Please fill in this form to create an account.</p>
        <hr>
        <label for="email"><b>FullName</b></label>
        <input type="text" placeholder="Enter FullName" ng-model="FullName" name="fullname" id="fullname" required>

        <label for="email"><b>Email</b></label>
        <input type="text" placeholder="Enter Email" ng-model="Email" name="email" id="email" required>

        <label for="psw"><b>Password</b></label>
        <input type="password" placeholder="Enter Password" ng-model="Password" name="Password" id="psw" required>


        <p>By creating an account you agree to our <a href="#">Terms & Privacy</a>.</p>

        <button ng-click="SignUp()" class="signUpbtn">Register</button>
    

    <div class="container signin">
        <p>Already have an account? <a href="#">Sign in</a>.</p>
    </div>
    <p>Output Message : {{msg}}</p>
    <p>StatusCode: {{statusval}}</p>
    <p>Status: {{statustext}}</p>
    <p>Response Headers: {{headers}}</p>

</div>
